{
  "name": "tactus-videos",
  "version": "1.0.0",
  "description": "Babulus videos about the Tactus programming language",
  "scripts": {
    "generate": "bash bin/babulus generate content/",
    "generate:watch": "bash bin/babulus generate content/ --watch",
    "generate:dev": "bash bin/babulus generate content/ --watch --env development",
    "generate:dry": "bash bin/babulus generate content/ --provider dry-run --sfx-provider dry-run --music-provider dry-run",
    "generate:fresh": "bash bin/babulus generate content/ --fresh",
    "generate:clean": "bash bin/babulus clean content/ --yes",
    "render": "node scripts/render-all.js",
    "render:intro": "node scripts/render-all.js intro",
    "sfx": "bash bin/babulus sfx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/tactus-videos.git"
  },
  "license": "MIT",
  "dependencies": {
    "babulus": "file:../../Babulus"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
