{
  "name": "tactus-videos",
  "version": "1.0.0",
  "description": "A collection of Remotion videos about the Tactus programming language",
  "scripts": {
    "start": "remotion studio",
    "build": "remotion render",
    "render": "node scripts/render-all.js",
    "render:intro": "remotion render src/index.ts Intro out/intro.mp4",
    "babulus:generate": "bash bin/babulus generate",
    "babulus:generate:fresh": "bash bin/babulus generate --fresh",
    "babulus:watch": "bash bin/babulus generate --watch",
    "babulus:clean": "bash bin/babulus clean",
    "babulus:clean:yes": "bash bin/babulus clean --yes",
    "babulus:sfx": "bash bin/babulus sfx",
    "upgrade": "remotion upgrade",
    "test": "eslint src --ext ts,tsx,js,jsx && tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/tactus-videos.git"
  },
  "license": "MIT",
  "dependencies": {
    "@remotion/bundler": "4.0.406",
    "@remotion/cli": "4.0.406",
    "@remotion/google-fonts": "4.0.406",
    "@remotion/player": "4.0.406",
    "js-yaml": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remotion": "4.0.406"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
