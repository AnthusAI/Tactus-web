        },
        {
          "type": "tts",
          "startSec": 348.2499999999999,
          "endSec": 353.5999999999999,
          "text": "Read the full story at tactus dot run slash guardrails.\n",
          "segmentPath": "/Users/ryan.porter/Projects/Tactus-web/videos/.babulus/out/guardrails/env/development/segments/closing--closing--tts--d7729fa6aeab--1.wav",
